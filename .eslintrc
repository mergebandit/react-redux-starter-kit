{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
   "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "react/require-extension": "off",
    "linebreak-style": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "comma-dangle": [1, "always-multiline"],
    "no-underscore-dangle": 0,
    "new-cap": 0
  }
}
